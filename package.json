{
  "name": "simple-single-spa-webpack-example",
  "version": "1.0.0",
  "description": "A simple example of how to use webpack with single-spa",
  "scripts": {
    "watch": "webpack --config webpack.config.js -w",
    "build": "webpack --config webpack.config.js -p && npm run build-children",
    "build-children": "cd ./src/app2 && webpack && cd ../app3 && webpack && cd ../..",
    "start": "http-server . -p 9090 -c 1"
  },
